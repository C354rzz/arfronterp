import { Component, OnDestroy, OnInit } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'page-with-boxed-layout',  templateUrl: './page-with-boxed-layout.html'})export class PageBoxedLayout implements OnInit, OnDestroy {  constructor(public appSettings: AppSettings, private http: HttpClient) {  }    ngOnInit() {    document.body.className = document.body.className + ' boxed-layout';  }  ngOnDestroy() {    document.body.className = document.body.className.replace('boxed-layout', '');  }}