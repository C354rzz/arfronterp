import { Component, OnDestroy, OnInit } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'page-with-two-sidebar',  templateUrl: './page-with-two-sidebar.html',})export class PageTwoSidebar implements OnInit, OnDestroy {  constructor(public appSettings: AppSettings) {  }    ngOnInit() {    this.appSettings.appSidebarTwo = true;    this.appSettings.appSidebarEndToggled = true;  }  ngOnDestroy() {    this.appSettings.appSidebarTwo = false;    this.appSettings.appSidebarEndToggled = false;  }}