import { Component, OnDestroy, OnInit } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'page-full-height',  templateUrl: './page-full-height.html'})export class PageFullHeight implements OnDestroy, OnInit {  constructor(public appSettings: AppSettings) {  }    ngOnInit() {    this.appSettings.appContentFullHeight = true;    this.appSettings.appContentClass = 'p-0';  }  ngOnDestroy() {    this.appSettings.appContentFullHeight = false;    this.appSettings.appContentClass = '';  }}